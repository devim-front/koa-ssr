{
  "name": "@devim-front/koa-ssr",
  "version": "0.0.1",
  "description": "Koa Middleware для Server Side Rendering приложений React",
  "main": "/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "types": "./build/index.d.ts",
  "files": [
    "/build",
    "/index.js"
  ],
  "scripts": {
    "build:drop": "rimraf build",
    "build:make": "tsc",
    "build": "run-s build:*",
    "test": "mocha -r ts-node/register ./src/index.test.ts",
    "release:make": "run-s build",
    "release:push": "np",
    "release:done": "npm publish",
    "release": "run-s release:*"
  },
  "keywords": [
    "devim-front",
    "react",
    "ssr",
    "koa"
  ],
  "author": "gleb-mikhalkov",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/koa": "^2.11.6",
    "@types/mocha": "^7.0.2",
    "@types/react-dom": "^16.9.9",
    "@types/supertest": "^2.0.10",
    "chai": "^4.2.0",
    "husky": "^4.2.5",
    "koa": "^2.13.1",
    "mocha": "^7.2.0",
    "np": "^6.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react": "^17.0.1",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devim-front/koa-ssr.git"
  },
  "bugs": {
    "url": "https://github.com/devim-front/koa-ssr/issues"
  },
  "homepage": "https://github.com/devim-front/koa-ssr#readme",
  "dependencies": {
    "htmlparser2": "^6.0.0"
  },
  "peerDependencies": {
    "koa": "^2.13.1",
    "react-dom": "^16.14.0",
    "react": "^17.0.1"
  }
}
